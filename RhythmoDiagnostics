                            <strong>–ó–∞–≤–æ–∂—É –•–ª–∞–¥–∞—Å—Ç–æ–π–Ω–æ–µ –ø–æ–ª–µ, –†–∞–¥–∞—Å—Ç–æ–π–Ω–æ–µ –ü–æ–ª–µ, –ú–∞—Ç—Ä–∏—á–Ω–æ–µ –ø–æ–ª–µ</strong>
                        </div>
                        <div style="background: linear-gradient(135deg, #FFD93D 0%, #FFC107 60%, #FFD93D 100%); border-radius: 15px; padding: 15px; color: #333; min-width: 200px; text-align: center;">
                            <strong>–ó–∞–≤–æ–∂—É –°—Ç—Ä–∞–¥–∞—Å—Ç–µ–π–Ω–æ–µ –ø–æ–ª–µ</strong>
                        </div>
                    </div>

                    <!-- –ó–µ–ª—ë–Ω—ã–µ –±–ª–æ–∫–∏ -->
                    <div style="display: flex; gap: 20px; margin-bottom: 20px; flex-wrap: wrap; justify-content: center;">
                        <div style="background: linear-gradient(135deg, #4CAF50 0%, #2E7D32 60%, #4CAF50 100%); border-radius: 15px; padding: 20px; color: white; min-width: 250px; text-align: center;">
                            <strong>–ü–µ—Ä–µ–¥ –≤—ã—Ö–æ–¥–æ–º –∏–∑ –¥–æ–º–∞ –æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ</strong><br>
                            <strong>–ß–∏—Ç–∞—é –≤—Å–µ –ù–æ–ª–∏ –∏ –ù—É–ª–∏</strong>
                        </div>
                        <div style="background: linear-gradient(135deg, #4CAF50 0%, #2E7D32 60%, #4CAF50 100%); border-radius: 15px; padding: 20px; color: white; min-width: 300px; text-align: center;">
                            <strong>–ß–∏—Ç–∞—é –≤—Å–µ –ü–µ—Ä–ª–∞–º—É—Ç—Ä–æ–≤—ã–µ –í—Ä–µ–º–µ–Ω–∞, –°—Ç—Ä—É–Ω—ã –∏ –°—Ç—Ä—É–∏</strong>
                        </div>
                        <div style="background: linear-gradient(135deg, #4CAF50 0%, #2E7D32 60%, #4CAF50 100%); border-radius: 15px; padding: 20px; color: white; min-width: 250px; text-align: center;">
                            <strong>–î–∞—Ä–∞–∫–æ–¥, –ù–∞–Ω–æ—Ö–æ–¥, –ú—Å–∏–æ–æ–ª—Ä, –°—Ç—Ä–∞–∑–∏—Ç</strong>
                        </div>
                    </div>

                    <!-- –ì–æ–ª—É–±—ã–µ –±–ª–æ–∫–∏ -->
                    <div style="display: flex; gap: 20px; margin-bottom: 20px; flex-wrap: wrap; justify-content: center;">
                        <div style="background: linear-gradient(135deg, #42A5F5 0%, #1976D2 60%, #42A5F5 100%); border-radius: 15px; padding: 20px; color: white; min-width: 280px; text-align: center;">
                            <strong>–ó–∞–≤–æ–∂—É –†–∞–¥–∞—Å—Ç–µ–π–Ω–æ–µ –∏ –°—É—Ç–µ–≤–æ–µ –ø–æ–ª—è</strong>
                        </div>
                        <div style="background: linear-gradient(135deg, #42A5F5 0%, #1976D2 60%, #42A5F5 100%); border-radius: 15px; padding: 20px; color: white; min-width: 280px; text-align: center;">
                            <strong>–í —Ç–µ—á–µ–Ω–∏–∏ –¥–Ω—è –æ—Ç–∫—Ä—ã–≤–∞—é –∫–Ω–∏–≥—É —Å –†–∏—Ç–º–æ–ª–æ–≥–∏–µ–π - –ª—é–±—É—é –ø–æ –∏–º–ø—É–ª—å—Å—É</strong>
                        </div>
                        <div style="background: linear-gradient(135deg, #42A5F5 0%, #1976D2 60%, #42A5F5 100%); border-radius: 15px; padding: 20px; color: white; min-width: 200px; text-align: center;">
                            <strong>–°–ª—É—à–∞—é —Ñ–∏–ª—å—Ç—Ä—ã</strong>
                        </div>
                    </div>

                    <!-- –°–∏–Ω–∏–µ –±–ª–æ–∫–∏ -->
                    <div style="display: flex; gap: 20px; margin-bottom: 20px; flex-wrap: wrap; justify-content: center;">
                        <div style="background: linear-gradient(135deg, #3F51B5 0%, #1A237E 60%, #3F51B5 100%); border-radius: 15px; padding: 20px; color: white; min-width: 280px; text-align: center;">
                            <strong>–í—Å—ë —ç—Ç–æ —è –º–æ–≥—É –¥–µ–ª–∞—Ç—å –≤ –¥–≤–∏–∂–µ–Ω–∏–∏ —É –º–µ–Ω—è –µ—Å—Ç—å –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –†–∏—Ç–º–æ–í—Ä–µ–º—è–¢–í</strong>
                        </div>
                        <div style="background: linear-gradient(135deg, #3F51B5 0%, #1A237E 60%, #3F51B5 100%); border-radius: 15px; padding: 20px; color: white; min-width: 200px; text-align: center;">
                            <strong>–ê–∫—Ç–∏–≤–∏–∑–∏—Ä—É—é —Å–≤–æ–∏ –ø–æ–¥–∞—Ä–∫–∏ –æ—Ç –û—Å–Ω–æ–≤–Ω–æ–≥–æ –õ—É—á–∞</strong>
                        </div>
                        <div style="background: linear-gradient(135deg, #3F51B5 0%, #1A237E 60%, #3F51B5 100%); border-radius: 15px; padding: 20px; color: white; min-width: 200px; text-align: center;">
                            <strong>–°–ª—É—à–∞—é —Å—Ç—Ä—É–Ω—ã —Ü–≤–µ—Ç–æ–≤—ã–µ, –∑–≤—É–∫–æ–≤—ã–µ, —á–∞—à–∏ –∏ —Å—Ñ–µ—Ä—ã</strong>
                        </div>
                    </div>

                    <!-- –§–∏–æ–ª–µ—Ç–æ–≤—ã–π –±–ª–æ–∫ -->
                    <div style="background: linear-gradient(135deg, #9C27B0 0%, #4A148C 60%, #9C27B0 100%); border-radius: 20px; padding: 30px; color: white; box-shadow: 0 8px 15px rgba(0,0,0,0.25); text-align: center; margin-top: 30px;">
                        <h2 style="font-size: 2.5em; margin-bottom: 10px;">üåü –Ø –°—á–∞—Å—Ç–ª–∏–≤–∞ üåü</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- –ù–∞–≤–∏–≥–∞—Ü–∏–æ–Ω–Ω—ã–µ —Å—Ç—Ä–µ–ª–∫–∏ -->
    <div class="nav-arrows">
        <button class="nav-arrow" id="prevBtn" onclick="previousSlide()">‚Üê</button>
        <button class="nav-arrow" id="nextBtn" onclick="nextSlide()">‚Üí</button>
    </div>

    <script>
        // –ì–ª–æ–±–∞–ª—å–Ω—ã–µ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ
        let currentSection = 'diagnostics';
        let currentSlides = {
            diagnostics: 0,
            ritmometers: 0,
            concepts: 0,
            'rhythm-hygiene': 0
        };
        const totalSlides = {
            diagnostics: 6,
            ritmometers: 2,
            concepts: 4,
            'rhythm-hygiene': 1
        };

        // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∫–∞–∑–∞ –æ—Å–Ω–æ–≤–Ω–æ–≥–æ —Ä–∞–∑–¥–µ–ª–∞
        function showMainSection(sectionName) {
            // –°–∫—Ä—ã—Ç—å –≤—Å–µ —Ä–∞–∑–¥–µ–ª—ã
            document.querySelectorAll('.content-section').forEach(section => {
                section.classList.remove('active');
            });
            
            // –ü–æ–∫–∞–∑–∞—Ç—å –≤—ã–±—Ä–∞–Ω–Ω—ã–π —Ä–∞–∑–¥–µ–ª
            document.getElementById(sectionName + '-section').classList.add('active');
            
            // –û–±–Ω–æ–≤–∏—Ç—å –∞–∫—Ç–∏–≤–Ω—É—é –∫–Ω–æ–ø–∫—É –≤ –≥–ª–∞–≤–Ω–æ–π –Ω–∞–≤–∏–≥–∞—Ü–∏–∏
            document.querySelectorAll('.main-nav-btn').forEach(btn => {
                btn.classList.remove('active');
            });
            event.target.classList.add('active');
            
            // –û–±–Ω–æ–≤–∏—Ç—å —Ç–µ–∫—É—â–∏–π —Ä–∞–∑–¥–µ–ª
            currentSection = sectionName;
            
            // –ü–æ–∫–∞–∑–∞—Ç—å –ø–µ—Ä–≤—ã–π —Å–ª–∞–π–¥ –Ω–æ–≤–æ–≥–æ —Ä–∞–∑–¥–µ–ª–∞
            if (sectionName === 'rhythm-hygiene') {
                // –î–ª—è —Ä–∏—Ç–º–æ-–≥–∏–≥–∏–µ–Ω—ã –ø–æ–∫–∞–∑—ã–≤–∞–µ–º –µ–¥–∏–Ω—Å—Ç–≤–µ–Ω–Ω—ã–π —Å–ª–∞–π–¥
                document.getElementById('rhythm-hygiene-slide-0').classList.add('active');
                currentSlides[sectionName] = 0;
            } else {
                showSlide(sectionName, 0);
            }
            
            // –û–±–Ω–æ–≤–∏—Ç—å –Ω–∞–≤–∏–≥–∞—Ü–∏–æ–Ω–Ω—ã–µ —Å—Ç—Ä–µ–ª–∫–∏
            updateNavigationArrows();
        }

        // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∫–∞–∑–∞ –∫–æ–Ω–∫—Ä–µ—Ç–Ω–æ–≥–æ —Å–ª–∞–π–¥–∞ –≤ —Ä–∞–∑–¥–µ–ª–µ
        function showSlide(sectionName, slideIndex) {
            // –°–∫—Ä—ã—Ç—å –≤—Å–µ —Å–ª–∞–π–¥—ã –≤ —Ç–µ–∫—É—â–µ–º —Ä–∞–∑–¥–µ–ª–µ
            document.querySelectorAll(`#${sectionName}-section .slide`).forEach(slide => {
                slide.classList.remove('active');
            });
            
            // –ü–æ–∫–∞–∑–∞—Ç—å –≤—ã–±—Ä–∞–Ω–Ω—ã–π —Å–ª–∞–π–¥
            document.getElementById(`${sectionName}-slide-${slideIndex}`).classList.add('active');
            
            // –û–±–Ω–æ–≤–∏—Ç—å –∞–∫—Ç–∏–≤–Ω—É—é –∫–Ω–æ–ø–∫—É –≤ –ø–æ–¥–º–µ–Ω—é
            document.querySelectorAll(`#${sectionName}-section .sub-nav-btn`).forEach(btn => {
                btn.classList.remove('active');
            });
            
            const targetBtn = document.querySelector(`#${sectionName}-section .sub-nav-btn:nth-child(${slideIndex + 1})`);
            if (targetBtn) {
                targetBtn.classList.add('active');
            }
            
            // –û–±–Ω–æ–≤–∏—Ç—å —Ç–µ–∫—É—â–∏–π —Å–ª–∞–π–¥
            currentSlides[sectionName] = slideIndex;
            
            // –û–±–Ω–æ–≤–∏—Ç—å –Ω–∞–≤–∏–≥–∞—Ü–∏–æ–Ω–Ω—ã–µ —Å—Ç—Ä–µ–ª–∫–∏
            updateNavigationArrows();
        }

        // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–µ—Ä–µ—Ö–æ–¥–∞ –∫ —Å–ª–µ–¥—É—é—â–µ–º—É —Å–ª–∞–π–¥—É
        function nextSlide() {
            const currentSlideIndex = currentSlides[currentSection];
            const maxSlides = totalSlides[currentSection];
            
            if (currentSlideIndex < maxSlides - 1) {
                showSlide(currentSection, currentSlideIndex + 1);
            }
        }

        // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–µ—Ä–µ—Ö–æ–¥–∞ –∫ –ø—Ä–µ–¥—ã–¥—É—â–µ–º—É —Å–ª–∞–π–¥—É
        function previousSlide() {
            const currentSlideIndex = currentSlides[currentSection];
            
            if (currentSlideIndex > 0) {
                showSlide(currentSection, currentSlideIndex - 1);
            }
        }

        // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏—è –Ω–∞–≤–∏–≥–∞—Ü–∏–æ–Ω–Ω—ã—Ö —Å—Ç—Ä–µ–ª–æ–∫
        function updateNavigationArrows() {
            const currentSlideIndex = currentSlides[currentSection];
            const maxSlides = totalSlides[currentSection];
            
            const prevBtn = document.getElementById('prevBtn');
            const nextBtn = document.getElementById('nextBtn');
            
            // –û–±–Ω–æ–≤–∏—Ç—å —Å–æ—Å—Ç–æ—è–Ω–∏–µ –∫–Ω–æ–ø–∫–∏ "–ù–∞–∑–∞–¥"
            prevBtn.disabled = currentSlideIndex === 0;
            
            // –û–±–Ω–æ–≤–∏—Ç—å —Å–æ—Å—Ç–æ—è–Ω–∏–µ –∫–Ω–æ–ø–∫–∏ "–í–ø–µ—Ä–µ–¥"
            nextBtn.disabled = currentSlideIndex === maxSlides - 1;
        }

        // –û–±—Ä–∞–±–æ—Ç–∫–∞ –Ω–∞–∂–∞—Ç–∏–π –∫–ª–∞–≤–∏—à
        document.addEventListener('keydown', function(e) {
            if (e.key === 'ArrowLeft') {
                previousSlide();
            } else if (e.key === 'ArrowRight') {
                nextSlide();
            }
        });

        // –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –∏–Ω—Ç–µ—Ä–∞–∫—Ç–∏–≤–Ω–æ—Å—Ç–∏ –∫ –∫–∞—Ä—Ç–æ—á–∫–∞–º
        document.addEventListener('DOMContentLoaded', function() {
            // –ê–Ω–∏–º–∞—Ü–∏—è –¥–ª—è –∫–∞—Ä—Ç–æ—á–µ–∫ –∫–æ–Ω—Ü–µ–ø—Ü–∏–π
            document.querySelectorAll('.concept-card').forEach(card => {
                card.addEventListener('click', function() {
                    this.style.transform = 'translateY(-10px) scale(1.02)';
                    setTimeout(() => {
                        this.style.transform = 'translateY(-10px)';
                    }, 200);
                });
            });
            
            // –ê–Ω–∏–º–∞—Ü–∏—è –¥–ª—è —É—Ä–æ–≤–Ω–µ–π
            document.querySelectorAll('.level').forEach(level => {
                level.addEventListener('click', function() {
                    this.style.boxShadow = '0 0 25px rgba(255, 215, 0, 0.6)';
                    this.style.borderLeftWidth = '8px';
                    setTimeout(() => {
                        this.style.boxShadow = '0 6px 20px rgba(0,0,0,0.15)';
                        this.style.borderLeftWidth = '5px';
                    }, 1000);
                });
            });

            // –ê–Ω–∏–º–∞—Ü–∏—è –¥–ª—è –∫–∞—Ä—Ç–æ—á–µ–∫ –≤–æ–ø—Ä–æ—Å–æ–≤
            document.querySelectorAll('.question-card').forEach(card => {
                card.addEventListener('mouseenter', function() {
                    this.style.background = 'linear-gradient(135deg, #f8f9ff 0%, #e8f0ff 100%)';
                });
                
                card.addEventListener('mouseleave', function() {
                    this.style.background = 'white';
                });
            });

            // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –Ω–∞–≤–∏–≥–∞—Ü–∏–æ–Ω–Ω—ã—Ö —Å—Ç—Ä–µ–ª–æ–∫
            updateNavigationArrows();
        });

        // –ü–ª–∞–≤–Ω–∞—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∞ –ø—Ä–∏ –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏–∏ —Å–ª–∞–π–¥–æ–≤
        function smoothScrollToTop() {
            window.scrollTo({
                top: 0,
                behavior: 'smooth'
            });
        }

        // –î–æ–±–∞–≤–ª—è–µ–º –ø–ª–∞–≤–Ω—É—é –ø—Ä–æ–∫—Ä—É—Ç–∫—É –∫ —Ñ—É–Ω–∫—Ü–∏—è–º –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è
        const originalShowSlide = showSlide;
        showSlide = function(sectionName, slideIndex) {
            originalShowSlide(sectionName, slideIndex);
            setTimeout(smoothScrollToTop, 100);
        };

        const originalShowMainSection = showMainSection;
        showMainSection = function(sectionName) {
            originalShowMainSection(sectionName);
            setTimeout(smoothScrollToTop, 100);
        };

        // –î–æ–±–∞–≤–ª—è–µ–º –ø–æ–¥–¥–µ—Ä–∂–∫—É —Å–≤–∞–π–ø–æ–≤ –¥–ª—è –º–æ–±–∏–ª—å–Ω—ã—Ö —É—Å—Ç—Ä–æ–π—Å—Ç–≤
        let startX = 0;
        let startY = 0;

        document.addEventListener('touchstart', function(e) {
            startX = e.touches[0].clientX;
            startY = e.touches[0].clientY;
        });

        document.addEventListener('touchend', function(e) {
            if (!startX || !startY) return;

            let endX = e.changedTouches[0].clientX;
            let endY = e.changedTouches[0].clientY;

            let diffX = startX - endX;
            let diffY = startY - endY;

            // –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –≥–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª—å–Ω—ã–π —Å–≤–∞–π–ø –±–æ–ª—å—à–µ –≤–µ—Ä—Ç–∏–∫–∞–ª—å–Ω–æ–≥–æ
            if (Math.abs(diffX) > Math.abs(diffY)) {
                if (Math.abs(diffX) > 50) { // –ú–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–ª—è —Å—Ä–∞–±–∞—Ç—ã–≤–∞–Ω–∏—è
                    if (diffX > 0) {
                        // –°–≤–∞–π–ø –≤–ª–µ–≤–æ - —Å–ª–µ–¥—É—é—â–∏–π —Å–ª–∞–π–¥
                        nextSlide();
                    } else {
                        // –°–≤–∞–π–ø –≤–ø—Ä–∞–≤–æ - –ø—Ä–µ–¥—ã–¥—É—â–∏–π —Å–ª–∞–π–¥
                        previousSlide();
                    }
                }
            }

            // –°–±—Ä–æ—Å –∑–Ω–∞—á–µ–Ω–∏–π
            startX = 0;
            startY = 0;
        });
    </script>
</body>
</html>
